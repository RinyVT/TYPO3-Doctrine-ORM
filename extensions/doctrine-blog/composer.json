{
  "name": "rinyvt/doctrine-blog",
  "type": "typo3-cms-extension",
  "description": "Blog extension based on Doctrine TYPO3",
  "license": "GPL-2.0-or-later",
  "require": {
    "php": "^8.2",
    "typo3/cms-core": "^13.3",
    "rinyvt/typo3-doctrine-orm": "*"
  },
  "authors": [
    {
      "name": "Riny van Tiggelen",
      "role": "Developer",
      "email": "info@online-gamer.nl",
      "homepage": "rinyvt.com"
    }
  ],
  "keywords": [
    "typo3",
    "doctrine",
    "orm",
    "blog"
  ],
  "autoload": {
    "psr-4": {
      "RinyVT\\DoctrineBlog\\": "Classes/"
    }
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "doctrine_blog"
    }
  }
}
