{
  "name": "typo3/cms-base-distribution",
  "description": "TYPO3 CMS Base Distribution",
  "license": "GPL-2.0-or-later",
  "repositories": [
    {
      "type": "path",
      "url": "extensions/*"
    }
  ],
  "config": {
    "platform": {
      "php": "8.1"
    },
    "sort-packages": true,
    "allow-plugins": {
      "typo3/class-alias-loader": true,
      "typo3/cms-composer-installers": true
    }
  },
  "require": {
    "rinyvt/doctrine-blog": "@dev",
    "rinyvt/sitepackage": "@dev",
    "rinyvt/typo3-doctrine-orm": "@dev",
    "typo3/cms-backend": "*",
    "typo3/cms-belog": "*",
    "typo3/cms-beuser": "*",
    "typo3/cms-core": "^12.4",
    "typo3/cms-dashboard": "*",
    "typo3/cms-extbase": "*",
    "typo3/cms-extensionmanager": "*",
    "typo3/cms-felogin": "*",
    "typo3/cms-filelist": "*",
    "typo3/cms-fluid": "*",
    "typo3/cms-fluid-styled-content": "*",
    "typo3/cms-form": "*",
    "typo3/cms-frontend": "*",
    "typo3/cms-impexp": "*",
    "typo3/cms-info": "*",
    "typo3/cms-install": "*",
    "typo3/cms-lowlevel": "*",
    "typo3/cms-recordlist": "*",
    "typo3/cms-rte-ckeditor": "*",
    "typo3/cms-seo": "*",
    "typo3/cms-setup": "*",
    "typo3/cms-sys-note": "*",
    "typo3/cms-t3editor": "*",
    "typo3/cms-tstemplate": "*",
    "typo3/cms-viewpage": "*"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
